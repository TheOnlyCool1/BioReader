{

    "name": "BioReader",
    "description": "Applies Bionic Reading to a page",
    "version": "0.0",
    "permissions": [
        "webNavigation",
        "scripting",
        "activeTab",
        "tabs",
        "memory"
    ],
    "action": {
        "default_title":  "Click to enable for this page."
    },
    "background": {
        "service_worker": "bolder.js"
    },
    "manifest_version": 3
}
